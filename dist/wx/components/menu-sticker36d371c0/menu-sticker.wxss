
.menu-sticker {
  height: 100%;
  overflow: auto;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.menu-sticker .item {
  box-sizing: border-box;
  float: left;
  width: 100rpx;
  height: 100rpx;
  border-radius: 5rpx;
  background: #eee;
  margin: 10rpx;
  position: relative;
  padding: 5rpx;
}
.menu-sticker .item .img-sticker {
  width: 100%;
  height: 100%;
}
