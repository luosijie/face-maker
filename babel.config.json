{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "shippedProposals": true,
        "targets": "ios >= 9, chrome >= 47"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/transform-runtime",
      {
        "corejs": 3
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "shippedProposals": true,
            "targets": "ios > 8, chrome > 52"
          }
        ]
      ]
    }
  }
}
